# docker alias

alias dk-img='docker images'
alias dk-ps='docker ps'
alias dk-stop='docker stop'
alias dk-rmi='docker rmi'
alias dk-image-prune='docker image prune'
alias dk-container-prune='docker container prune'

alias dk-run='docker run -it --rm'
alias dk-rm-all-cotainer="docker ps -a | awk '{print $1}' | grep -v CONTAINER | xargs docker rm"
